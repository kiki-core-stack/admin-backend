{
  "private": true,
  "type": "module",
  "scripts": {
    "build": "bun run build:exec --bytecode --outdir=./dist ./src/index.ts",
    "build:exec": "NODE_ENV=production bun --env-file=./.env.production.local build --minify --target bun",
    "build:optimization": "./build.sh --clean",
    "compile": "bun run build:exec --bytecode --compile --outfile ./dist/index ./src/index.ts",
    "compile:optimization": "./build.sh --clean --compile",
    "dev": "NODE_ENV=development bun --env-file=./.env.development.local --watch run ./src/index.ts",
    "type-check": "tsc"
  },
  "dependencies": {
    "@kikiutils/kiki-core-stack-pack": "^0.22.1",
    "hono": "^4.6.3"
  },
  "devDependencies": {
    "@kikiutils/prettierrc": "^1.3.2",
    "@kikiutils/tsconfigs": "^3.0.1",
    "@types/bun": "^1.1.10",
    "ts-project-builder": "^3.1.0",
    "tslib": "^2.7.0",
    "typescript": "^5.6.3"
  },
  "prettier": "@kikiutils/prettierrc"
}
